{
	"name": "glthemes/constructionn-pro",
	"type": "package",
	"description": "Constructionn Pro",
	"keywords": [
		"WordPress",
		"Themes"
	],
	"homepage": "https://github.com/WordPress/twentytwenty",
	"license": "GPL-2.0-or-later",
	"authors": [
        {
            "name": "glthemes",
            "email": "binaglthemepradhan@gmail.com"
        }
    ],
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.10",
		"wp-coding-standards/wpcs": "^3.1"
	},
    "config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"scripts": {
		"format": "phpcbf --standard=phpcs.xml.dist --report-summary --report-source",
		"lint": "phpcs --standard=phpcs.xml.dist"
	}
}


	
	